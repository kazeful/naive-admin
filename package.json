{
  "name": "naive-admin",
  "version": "0.0.0",
  "author": "Wind",
  "scripts": {
    "postinstall": "pnpm link --global naive-directives",
    "build": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "dev": "vue-cli-service serve",
    "lint": "eslint . --ext .js,.vue",
    "preview": "node script/index.js --preview",
    "report": "node script/index.js --preview --report",
    "tinify": "node script/tinify.js",
    "svgo": "pnpm dlx svgo -f src/assets/icons/svg",
    "test:br": "pnpm dlx http-server dist --cors --brotli -c-1",
    "test:gzip": "pnpm dlx http-server dist --cors --gzip -c-1"
  },
  "dependencies": {
    "@riophae/vue-treeselect": "^0.4.0",
    "axios": "~0.19.2",
    "core-js": "^3.30.0",
    "dayjs": "^1.11.7",
    "element-ui": "^2.15.13",
    "js-cookie": "~2.2.0",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "nprogress": "~0.2.0",
    "path-to-regexp": "2.4.0",
    "screenfull": "^5.2.0",
    "vue": "^2.7.15",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@antfu/eslint-config-basic": "^0.38.4",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "body-parser": "^1.20.2",
    "chalk": "~2.4.2",
    "chokidar": "3.5.3",
    "compression-webpack-plugin": "^6.1.1",
    "connect": "^3.6.6",
    "eslint": "^8.38.0",
    "eslint-plugin-vue": "^9.10.0",
    "mockjs": "^1.1.0",
    "qs": "^6.11.1",
    "runjs": "~4.3.2",
    "sass": "~1.26.8",
    "sass-loader": "~8.0.2",
    "serve-static": "^1.15.0",
    "svg-baker-runtime": "~1.4.7",
    "svg-sprite-loader": "~4.1.3",
    "tinify": "^1.7.1",
    "vue-cli-plugin-windicss": "~1.1.6",
    "webpack": "^4.46.0"
  }
}
