{
  "name": "naive-admin",
  "version": "0.0.0",
  "author": "Wind",
  "scripts": {
    "postinstall": "pnpm link --global naive-directives",
    "build": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "dev": "vue-cli-service serve",
    "lint": "eslint .",
    "preview": "node script/index.js --preview",
    "report": "node script/index.js --preview --report",
    "tinify": "node script/tinify.js",
    "svgo": "pnpm dlx svgo -f src/assets/icons/svg",
    "test:br": "pnpm dlx http-server dist --cors --brotli -c-1",
    "test:gzip": "pnpm dlx http-server dist --cors --gzip -c-1"
  },
  "dependencies": {
    "@riophae/vue-treeselect": "^0.4.0",
    "@vueuse/core": "^10.7.2",
    "axios": "~1.6.5",
    "core-js": "^3.35.1",
    "dayjs": "^1.11.10",
    "element-ui": "^2.15.14",
    "js-cookie": "~3.0.5",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "nprogress": "~0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "2.4.0",
    "screenfull": "^6.0.2",
    "vue": "^2.7.15",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.26.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "body-parser": "^1.20.2",
    "chalk": "~2.4.2",
    "chokidar": "~3.5.3",
    "compression-webpack-plugin": "^11.0.0",
    "connect": "^3.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "mockjs": "^1.1.0",
    "runjs": "~4.4.2",
    "sass": "~1.32.13",
    "sass-loader": "^14.0.0",
    "serve-static": "^1.15.0",
    "svg-sprite-loader": "~6.0.11",
    "tinify": "^1.7.1",
    "vue-cli-plugin-windicss": "~1.1.6"
  }
}
